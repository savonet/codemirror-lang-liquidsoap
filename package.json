{
  "name": "lezer-liquidsoap",
  "version": "0.1.0",
  "description": "lezer-based liquidsoap grammar",
  "main": "dist/index.cjs",
  "bin": {
    "liquidsoap-lezer-print-tree": "dist/cli.js"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Romain Beauxis <toots@rastageeks.org>",
  "license": "MIT",
  "devDependencies": {
    "@lezer/generator": "^1.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup": "^2.52.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@lezer/common": "^1.1.0",
    "@lezer/highlight": "^1.1.3",
    "@lezer/lr": "^1.3.0",
    "@types/node": "^20.8.7",
    "bluebird": "^3.7.2",
    "glob": "^10.3.10",
    "minimist": "^1.2.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/savonet/lezer-liquidsoap.git"
  },
  "scripts": {
    "build": "lezer-generator src/liquidsoap.grammar -o src/parser && rollup -c && tsc",
    "build-debug": "lezer-generator src/liquidsoap.grammar --names -o src/parser && rollup -c && tsc",
    "test": "./tests/run.sh",
    "prepare": "npm run build"
  }
}
